@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .

doap:Project
    doap:name 'Bjartur' ;
    doap:homepage "https://github.com/ealtamir/Bjartur" ;
    doap:shortdesc "A simple SPDY-based http server in node.js" ;
    doap:maintainer _:enzo ;
    doap:repository _:repo .

_:enzo a foaf:Person ;
    foaf:name "Enzo Altamiranda" ;
    foaf:mbox "enzo.alt@gmail.com" ;
    foaf:homepage "https://github.com/ealtamir" .

_:repo a doap:GitRepository .
